{
	"name": "PL_FactTable_Invoicing_SAP_DL",
	"properties": {
		"activities": [
			{
				"name": "QUERY_SAP_TO_DATALAKE",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT \nDT.FKDAT AS FECHA_FACTURACION,\nDT.MENGE AS CANTIDAD_VENDIDA,\nDT.FAKWR AS MONTO_FACTURADO,\nLFA1.NAME1 AS NOMBRE_PROVEEDOR\n\nFROM\n\ngta.INV_HEADER HD\nINNER JOIN gta.INVOICING DT ON HD.FPLNR = DT.FPLNR \nLEFT JOIN gta.KNB1 ON DT.BUKRS = KNB1.BUKRS\nLEFT JOIN gta.LFA1 ON DT.LIFNR = LFA1.LIFNR\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_ORIGEN_QUERY",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_DESTINO_DATALAKE",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}